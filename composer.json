{
    "name": "fitzwilliammuseum/fitz-main-website",
    "type": "project",
    "description": "A front end for the Fitzwilliam Museum website",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "git",
            "name": "fitzwilliammuseum/maps",
            "url": "https://github.com/fitzwilliammuseum/maps.git"
        },
        {
            "type": "git",
            "name": "fitzwilliammuseum/instagram-php-scraper",
            "url": "https://github.com/fitzwilliammuseum/instagram-php-scraper.git"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "alaouy/youtube": "^2.2.5",
        "arcanedev/laravel-markdown": "^6.0",
        "atymic/twitter": "^3.1",
        "codeat3/blade-fluentui-system-icons": "^1.31.1",
        "conedevelopment/blade-filters": "^1.0",
        "diglactic/laravel-breadcrumbs": "^v8.0.1",
        "elasticsearch/elasticsearch": "^7.17.1",
        "fitzwilliammuseum/instagram-php-scraper": "master",
        "fitzwilliammuseum/maps": "master",
        "fruitcake/laravel-cors": "^3.0.0",
        "gmostafa/php-graphql-client": "^1.13",
        "guzzlehttp/guzzle": "^7.5.0",
        "imagekit/imagekit": "^2.0.0",
        "imliam/laravel-blade-helper": "^1.4.3",
        "jmikola/geojson": "^1.1.1",
        "laravel/framework": "^9.37",
        "laravel/tinker": "^2.7.2",
        "laravel/ui": "^3.4.6",
        "laravelcollective/html": "^6.3",
        "mews/purifier": "^3.3.8",
        "olssonm/l5-very-basic-auth": "^6.8",
        "owenvoke/blade-fontawesome": "^2.1.0",
        "phpclassic/php-shopify": "^v1.2.3",
        "phpfastcache/phpfastcache": "^9.1.2",
        "predis/predis": "^1.1.10",
        "psr/cache": "^3.0",
        "rebing/graphql-laravel": "^8.3",
        "solarium/solarium": "^6.2.7",
        "spatie/laravel-ignition": "^1.6.1",
        "spatie/laravel-feed": "^4.1.5",
        "spatie/laravel-googletagmanager": "^2.6.6",
        "spatie/laravel-missing-page-redirector": "^2.9.3",
        "spatie/laravel-responsecache": "^7.4.3",
        "spatie/laravel-sitemap": "^6.2.3",
        "symfony/cache": "^6.1.7",
        "symfony/http-kernel": "^6.2.x-dev",
        "upthemedia/xss-protection": "^0.0.8",
        "xantios/mimey": "^2.2.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12.3",
        "fakerphp/faker": "^1.20.0",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^6.3.1",
        "phpunit/phpunit": "^9.5.26",
        "sven/artisan-view": "^3.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
