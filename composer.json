{
    "name": "fitzwilliammuseum/fitz-main-website",
    "type": "project",
    "description": "A front end for the Fitzwilliam Museum website",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "git",
            "name": "fitzwilliammuseum/maps",
            "url": "https://github.com/fitzwilliammuseum/maps.git"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "alaouy/youtube": "^2.2.5",
        "atymic/twitter": "^3.1",
        "codeat3/blade-fluentui-system-icons": "^1.30.1",
        "conedevelopment/blade-filters": "^1.0",
        "diglactic/laravel-breadcrumbs": "^v10.0.0",
        "elasticsearch/elasticsearch": "^7.17.0",
        "fitzwilliammuseum/maps": "master",
        "gmostafa/php-graphql-client": "^1.13",
        "guzzlehttp/guzzle": "^7.4.5",
        "imliam/laravel-blade-helper": "^1.6.0",
        "jmikola/geojson": "^1.1.1",
        "laravel/framework": "^11.0",
        "laravel/tinker": "^2.7.2",
        "laravel/ui": "^4.0.0",
        "mews/purifier": "^3.3.8",
        "olssonm/l5-very-basic-auth": "^7.2",
        "owenvoke/blade-fontawesome": "^2.0",
        "phpclassic/php-shopify": "^v1.1.24",
        "phpfastcache/phpfastcache": "^9.1.2",
        "predis/predis": "^3.2.0",
        "psr/cache": "^3.0",
        "solarium/solarium": "^6.2.6",
        "spatie/laravel-feed": "^4.1.4",
        "spatie/laravel-googletagmanager": "^2.6.6",
        "spatie/laravel-html": "^3.12",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-markdown": "^2.7.1",
        "spatie/laravel-missing-page-redirector": "^2.9.2",
        "spatie/laravel-responsecache": "^7.4.1",
        "spatie/laravel-sitemap": "^7.3.6",
        "symfony/cache": "^7.3.2",
        "symfony/http-kernel": "^7.3.2",
        "upthemedia/xss-protection": "^0.0.8",
        "xantios/mimey": "^2.2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^3.6.0",
        "fakerphp/faker": "^1.20.0",
        "mockery/mockery": "^1.5.0",
        "nunomaduro/collision": "^8.8.2",
        "phpunit/phpunit": "^11.5.33"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
