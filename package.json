{
    "private": true,
    "scripts": {
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "production": "mix --production",
        "copy": "npm run copy-uv-html && npm run copy-uv-css",
        "copy-uv-html": "cp -R node_modules/universalviewer/dist/uv.html ./public",
        "copy-uv-css": "cp -R node_modules/universalviewer/dist/uv.css ./public/",
        "postinstall": "npm run copy"
    },
    "devDependencies": {
        "ansi-regex": "^6.0.1",
        "async": "^3.2.3",
        "axios": "^0.27.2",
        "bootstrap": "^5.1.3",
        "cross-env": "^7.0.3",
        "glob-parent": "^6.0.2",
        "is-svg": "^4.3.2",
        "jquery": "^3.6.0",
        "laravel-mix": "^6.0.43",
        "lodash": "^4.17.21",
        "node-forge": "^1.3.1",
        "nth-check": ">=2.0.1",
        "object-path": ">=0.11.8",
        "postcss": "^8.4.13",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.51.0",
        "sass-loader": "^12.6.0",
        "serialize-javascript": "^6.0.0",
        "ssri": "^9.0.0",
        "url-parse": "^1.5.10",
        "vue-template-compiler": "^2.6.14",
        "websocket-extensions": ">=0.1.4"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.1.1",
        "@popperjs/core": "^2.11.5",
        "leaflet-iiif": "^3.0.0",
        "universalviewer": "^4.0.0-pre.105"
    }
}
